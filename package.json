{
  "name": "unoapi",
  "version": "1.0.0",
  "private": true,
  "description": "前端API自动化编码工具",
  "scripts": {
    "build": "rimraf packages/*/dist -g && pnpm -r build",
    "local:changeset": "pnpm changeset",
    "local:version": "pnpm changeset version",
    "local:publish": "pnpm changeset publish --no-git-checks",
    "ci:publish": "pnpm build && pnpm changeset publish"
  },
  "keywords": [],
  "author": "moohng@前端星河",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "2.29.7",
    "@types/node": "24.3.1",
    "@unoapi/core": "workspace:*",
    "rimraf": "6.0.1",
    "typescript": "5.9.2"
  }
}
