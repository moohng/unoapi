  "components": {
    "schemas": {
      "CreateCatDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "The name of the cat"
          },
          "age": {
            "type": "number",
            "description": "The age of the cat in years",
            "minimum": 3
          },
          "breed": {
            "type": "string",
            "description": "The breed of the cat",
            "maxLength": 50
          }
        },
        "required": [
          "name",
          "age",
          "breed"
        ]
      },
      "Cat": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Whiskers",
            "description": "The name of the cat"
          },
          "age": {
            "type": "number",
            "example": 3,
            "description": "The age of the cat in years"
          },
          "breed": {
            "type": "string",
            "example": "Tabby",
            "description": "The breed of the cat"
          }
        },
        "required": [
          "name",
          "age",
          "breed"
        ]
      },
      "User": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number",
            "description": "用户ID"
          },
          "name": {
            "type": "string",
            "description": "用户名"
          },
          "email": {
            "type": "string",
            "description": "邮箱"
          },
          "avatarUrl": {
            "type": "string",
            "description": "头像地址"
          }
        },
        "required": [
          "id",
          "name"
        ]
      },
      "UserCreate": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "用户名"
          },
          "password": {
            "type": "string",
            "description": "密码"
          },
          "email": {
            "type": "string",
            "description": "邮箱"
          }
        },
        "required": [
          "name",
          "password"
        ]
      },
      "UserUpdate": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "用户名"
          },
          "email": {
            "type": "string",
            "description": "邮箱"
          }
        }
      },
      "ResetPasswordRequest": {
        "type": "object",
        "properties": {
          "newPassword": {
            "type": "string",
            "description": "新密码"
          }
        },
        "required": [
          "newPassword"
        ]
      },
      "LoginRequest": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "邮箱"
          },
          "password": {
            "type": "string",
            "description": "密码"
          }
        },
        "required": [
          "email",
          "password"
        ]
